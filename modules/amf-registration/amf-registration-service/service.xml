<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.liferay.registration" mvcc-enabled="true">
	<author>quanhuynh</author>
	<namespace>REGIS</namespace>
	<entity name="UserEntry" local-service="true" uuid="true" remote-service="true">

		<column name="userEntryId" type="long" primary="true"/>
		<column name="homePhone" type="String"/>
		<column name="mobilePhone" type="String"/>
		<column name="address1" type="String"/>
		<column name="address2" type="String"/>
		<column name="city" type="String"/>
		<column name="state" type="String"/>
		<column name="zipCode" type="String"/>
		<column name="userId" type="long"/>

	</entity>
</service-builder>